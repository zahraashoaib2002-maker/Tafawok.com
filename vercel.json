{
  "version": 2,
  "cleanUrls": true,
  "redirects": [
    { "source": "/register", "destination": "/register.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js|jpg|png|svg)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }]
    }
  ]
}
